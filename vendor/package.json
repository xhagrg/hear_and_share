{
  "name": "music_mania",
  "version": "1.1.0",
  "scripts": {
    "clean": "rimraf node_modules doc typings && npm cache clean",
    "clean-install": "npm run clean && npm install",
    "watch": "webpack --watch --progress --profile --colors --display-error-details --display-cached",
    "build": "webpack --progress --profile --colors --display-error-details --display-cached",
    "build:prod": "webpack --progress --profile --colors --display-error-details --display-cached --optimize-occurence-order --optimize-minimize --optimize-dedupe",
    "webdriver-update": "webdriver-manager update",
    "webdriver-start": "webdriver-manager start",
    "lint": "tsconfig-lint",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "docs": "typedoc  --options typedoc.json  src/**/*.ts",
    "clean-start": "rm -rf public/assets/* && npm run watch",
    "start": "npm run clean-start",
    "typings": "typings"
  },
  "dependencies": {
    "@angular/common": "^2.4.10",
    "@angular/compiler": "^2.4.10",
    "@angular/core": "^2.4.10",
    "@angular/forms": "^2.4.10",
    "@angular/http": "^2.4.10",
    "@angular/platform-browser": "^2.4.10",
    "@angular/platform-browser-dynamic": "^2.4.10",
    "@angular/router": "^3.4.10",
    "@ng-bootstrap/ng-bootstrap": "1.0.0-alpha.22",
    "angular2-loaders-css": "1.0.7",
    "core-js": "^2.4.1",
    "ng2-tooltip": "0.0.4",
    "ng2-youtube-player": "0.0.2",
    "ngx-modal": "0.0.29",
    "angular2-toaster": "^3.0.1",
    "reflect-metadata": "0.1.8",
    "notifyjs": "^3.0.0",
    "jquery": "^3.2.1",
    "toastr": "^2.1.2",
    "rxjs": "^5.3.0",
    "zone.js": "^0.7.2"
  },
  "devDependencies": {
    "angular2-template-loader": "^0.4.0",
    "assets-webpack-plugin": "latest",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "jasmine-core": "^2.4.1",
    "karma": "^1.6.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "ng2-youtube-player": "0.0.2",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.7",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "style-loader": "^0.13.2",
    "toastr": "^2.1.2",
    "ts-loader": "^0.8.1",
    "typescript": "^2.3.0",
    "typings": "^1.3.2",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.16.3",
    "webpack-merge": "^0.14.0"
  }
}
